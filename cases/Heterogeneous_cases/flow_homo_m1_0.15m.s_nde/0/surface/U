FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.15 0 0);

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    left 
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    top
    {
        type            slip;
    }
    bottom
    {
        //type            fixedValue;
        type            zeroNetFluxVelocity;
        value           uniform (0 0 0);
    }
}


