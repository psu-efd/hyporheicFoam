FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    left 
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    top
    {
        type            slip;
    }
    bottom
    {
        type            surfaceBottom;
        CFromSub        uniform 0;
        phi             phi;
        value           uniform 0;
    }
}


