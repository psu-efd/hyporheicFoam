FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue h 0
);

regions
(
    boxToCell
    {
        box (0 0 -1.0) (4 5 0);

        fieldValues
        (
            volScalarFieldValue h  0.5
        );
    }
);

