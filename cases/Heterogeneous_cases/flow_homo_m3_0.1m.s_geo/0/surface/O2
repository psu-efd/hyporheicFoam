FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "10";
    object      O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.27;   //mmol/L

boundaryField
{
    left
    {
        type            cyclic;
    }
    bottom
    {
        type            chesurfaceBottom;
        YiFromSub       uniform 0;
        phi             phi;
        value           uniform 0;
    }
   top
    {
        type            surfaceReaeration;
        Cs              uniform 0.27;
        K_L             uniform 1e-06;
        value           uniform 0;
    }
    right
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


