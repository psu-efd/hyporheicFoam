FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/surface";
    object      NO3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.16;

boundaryField
{
    bottom
    {
        type            chesurfaceBottom;
        YiFromSub       uniform 0;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 0;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
    top
    {
        type            slip;
    }
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
  
}


