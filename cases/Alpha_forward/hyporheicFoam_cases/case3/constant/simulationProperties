FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      simulationProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//whether to solve flow
//on: flow (both surface and subsurface) will be simulated
//off: flow will not be simulated (an option to use pre-simulated flow field)
flowSwitch on;

//maximum subiterations
maxCoupleIter 1;
